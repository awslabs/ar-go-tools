+package datastream
+
+import (
+	"bytes"
+    ctx "github.com/aws/amazon-ssm-agent/agent/context"
+    logger "github.com/aws/amazon-ssm-agent/agent/log"
+	mgsContracts "github.com/aws/amazon-ssm-agent/agent/session/contracts"
