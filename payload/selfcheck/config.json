{


  "options": {
    "log-level": 3,
    "pkg-filter": "ar-go-tools",
    "max-alarms": 2,
    "unsafe-max-depth": 10,
    "max-entrypoint-context-size": 2,
    "summarize-on-demand": true,
    "report-paths": true,
  },
  "dataflow-specs": [
    "argot-specs.json",
    "std-specs.json"
  ],
  "taint-tracking-problems": [
    {
      "sources": [
        {
          "package": "ssa",
          "context": "ar-go-tools",
          "method": "String"
        }
      ],
      "sinks": [
        {
          "context": "ar-go-tools/analysis",
          "package": "fmt",
          "method": "((Printf)|(Fprintf))"
        },
        {
          "package": "config",
          "method": "((Warnf)|(Debugf)|(Infof)|(Errorf))"
        }
      ]
    }
  ]
}